@model Pastures2019.Models.Soob
@using Pastures2019.Controllers
@using Microsoft.Extensions.Localization
@inject IStringLocalizer<SharedResources> SharedLocalizer
@{
    ViewData["Title"] = SharedLocalizer["Delete"];
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <div class="container">
        <h1>@SharedLocalizer["Delete"]</h1>

        <h3>@SharedLocalizer["Delete"]?</h3>
        <div>
            <h4>@SharedLocalizer["Soob"]</h4>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Code)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Code)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Description)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Description)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.DescriptionLat)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.DescriptionLat)
                </dd>
            </dl>

            <form asp-action="Delete">
                <input type="hidden" asp-for="Id" />
                <input type="submit" value="@SharedLocalizer["Delete"]" class="btn btn-danger" /> |
                <a asp-action="Index">@SharedLocalizer["BackToList"]</a>
            </form>
        </div>

    </div>
